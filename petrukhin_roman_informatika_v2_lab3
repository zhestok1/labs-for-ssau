#include <iostream>
#include <vector>
using namespace std;
// Функция для нахождения НОД двух чисел
int gcd(int a, int b) {
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}
// Функция для нахождения НОД для массива чисел
int findGCD(const vector<int>& numbers) {
    int result = numbers[0];
    for (size_t i = 1; i < numbers.size(); ++i) {
        result = gcd(result, numbers[i]);
        if (result == 1) {
            return 1; // НОД не может быть меньше 1
        }
    }
    return result;
}
int main() {
    vector<int> numbers;
    int n, num;
    cout << "Введите количество чисел: ";
    cin >> n;
    cout << "Введите числа: ";
    for (int i = 0; i < n; ++i) {
        cin >> num;
        numbers.push_back(num);
    }
    if (numbers.empty()) {
        cout << "Нет чисел для вычисления НОД." << endl;
        return 0;
    }
    int result = findGCD(numbers);
    cout << "Наибольший общий делитель: " << result << endl;
    return 0;
}
